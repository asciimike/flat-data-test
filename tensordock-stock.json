    <div id="toastContainer" class="toast-container position-fixed top-0 right-0 p-3" style="z-index: 9999;">
        <div id="exampleToast">
            <div id="toast_id" class="toast hide" role="alert" aria-live="assertive" aria-atomic="true"
                data-delay="10000">
                <div class="toast-header bg-toast_color">
                    <strong class="mr-auto text-dark">toast_title</strong>
                    <button type="button" class="btn-close" data-dismiss="toast" aria-label="Close"></button>
                </div>
                <div class="toast-body">
                    toast_body
                </div>
            </div>
        </div>

    </div>

    <script src="/static/js/vendor.bundle.js"></script>
    <script src="/static/js/theme.bundle.js"></script>
    <script>
        window.onload = (event) => {
            fadeInPage();
        };

        // Fade in page
        function fadeInPage() {
            if (!window.AnimationEvent) {
                return;
            }
            var fader = document.getElementById('fader');
            fader.classList.add('fade-out');
            setTimeout(function () {
                document.getElementById('slider').style.display = "none"
            }, 300);
        }

        /* Fade in page if back button was used */
        (function () {
            window.onpageshow = function (event) {
                if (event.persisted) {
                    fadeInPage()
                }
            };
        })();

        document.addEventListener('DOMContentLoaded', function () {
            if (!window.AnimationEvent) {
                return;
            }
            var anchors = document.getElementsByTagName('a');

            for (var idx = 0; idx < anchors.length; idx += 1) {
                if (anchors[idx].hostname !== window.location.hostname ||
                    anchors[idx].pathname === window.location.pathname ||
                    anchors[idx].target === "_blank") {
                    continue;
                }
                anchors[idx].addEventListener('click', function (event) {
                    document.getElementById('slider').style.display = "block";
                    var fader = document.getElementById('fader'),
                        anchor = event.currentTarget;

                    var listener = function () {
                        window.location = anchor.href;
                        fader.removeEventListener('animationend', listener);
                    };
                    fader.addEventListener('animationend', listener);

                    event.preventDefault();
                    fader.classList.add('fade-in');
                });
            }
        });

        window.addEventListener('pageshow', function (event) {
            if (!event.persisted) {
                return;
            }
            var fader = document.getElementById('fader');
            fader.classList.remove('fade-in');
        });

        // For Toasts
        function makeid(length) {
            var result = '';
            var characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
            var charactersLength = characters.length;
            for (var i = 0; i < length; i++) {
                result += characters.charAt(Math.floor(Math.random() *
                    charactersLength));
            }
            return result;
        }


        function showNotification(title, body, color) {
            var id = makeid(50);
            var toast = document.createElement("div");
            toast.innerHTML = document.getElementById("exampleToast").innerHTML.replace("toast_title", title).replace("toast_body", body).replace("toast_color", color).replace("toast_id", id);
            document.getElementById("toastContainer").appendChild(toast);

            var toastElList = [].slice.call(document.querySelectorAll('.toast'))
            var toastList = toastElList.map(function (toastEl) {
                return new bootstrap.Toast(toastEl)
            })

            console.log(toast);

            bootstrap.Toast.getInstance(document.getElementById(id)).show();
        }

        function showAlert(message) {
            showNotification("Alert", message, "warning")
        }

        function showError(message) {
            showNotification("Error", message, "danger")
        }

        function showSuccess(message) {
            showNotification("Success", message, "light")
        }

        function updateDepositButton() {
            var depositValue = parseInt(document.getElementById('fundsDepositAmount').value);
            console.log(depositValue);
            if (depositValue < 5) {
                replaceClass("depositFundsBtn", "btn-primary", "btn-secondary");
            } else {
                replaceClass("depositFundsBtn", "btn-secondary", "btn-primary");
            }
        }

        function updatePromoButton() {
            var promoValue = document.getElementById('promoCodeInput').value;
            console.log(promoValue);
            if (promoValue.trim().length == 0) {
                replaceClass("promoCodeBtn", "btn-primary", "btn-secondary");
            } else {
                replaceClass("promoCodeBtn", "btn-secondary", "btn-primary");
            }
        }


    </script>

    <script src="https://js.stripe.com/v3/"></script>
    <script type="text/javascript">
        // Stripe public key here
        var stripe = Stripe('pk_live_51JqNQYHSshR0IOtvfpFJ335VCKxHeyzKzGT8XaWMNvt5ye74VXApsofamVwZN3Ec2H9Y9Ap5WsVlVwxgnEL1Ys7R00rbTg3ky9');
        var depositValue = document.getElementById('fundsDepositAmount').value;
        var checkoutButton = document.getElementById("depositFundsBtn");
        checkoutButton.addEventListener("click", function () {
            var depositValue = document.getElementById('fundsDepositAmount').value;
            if (depositValue >= 5) {
                fetch("/createDepositFundsSession/" + document.getElementById("fundsDepositAmount").value, {
                    method: "POST",
                })
                    .then(function (response) {
                        return response.json();
                    })
                    .then(function (session) {
                        return stripe.redirectToCheckout({
                            sessionId: session.id
                        });
                    })
                    .then(function (result) {
                        // If redirectToCheckout fails due to a browser or network
                        // error, you should display the localized error message to your
                        // customer using error.message.
                        if (result.error) {
                            alert(result.error.message);
                        }
                    })
                    .catch(function (error) {
                        console.error("Error:", error);
                    });
            }
        });

        var promoCodeButton = document.getElementById("promoCodeBtn");
        promoCodeButton.addEventListener("click", function () {
            if (document.getElementById("promoCodeInput").value.trim().length != 0) {
                var code = document.getElementById('promoCodeInput').value;
                const Http = new XMLHttpRequest();
                const url = '/promo_code/' + code;
                Http.open("POST", url);
                Http.send();
                Http.onreadystatechange = (e) => {
                    if (Http.readyState === 4) {
                        if (Http.status == 200) {
                            window.location.assign(window.location.pathname + "?promoSuccess")
                        } else {
                            window.location.assign(window.location.pathname + "?promoFailed")
                        }
                    }
                }
            }
        });


        function replaceClass(id, oldClass, newClass) {
            var elem = document.getElementById(id);
            if (elem.classList.contains(oldClass)) {
                elem.classList.remove(oldClass);
                elem.classList.add(newClass);
            }
        }


        function initNotification() {
            if (location.search.includes("promoSuccess")) {
                showSuccess('Your promo code has been applied!');
            }
            if (location.search.includes("promoFailed")) {
                showError("Your promo code has failed. Make sure the code is valid, isn't expired, and that you haven't used it before. Email us at support@tensordock.com if you have any questions.");
            }
        }
        $(document).ready(function () {
            initNotification();
        });
    </script>
